{
  "name": "@strivacity/sdk-js",
  "version": "0.0.1",
  "description": "Strivacity JavaScript SDK",
  "license": "MIT",
  "author": "strivacity<info@strivacity.com>",
  "main": "./dist/strivacity-sdk.cjs.js",
  "unpkg": "./dist/strivacity-sdk.umd.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "yarn run build",
    "prebuild": "node ./scripts/rm.js ./dist",
    "build": "yarn run build:native && yarn run build:bundle",
    "build:native": "tsc",
    "build:bundle": "rollup -c",
    "lint": "yarn run eslint --format codeframe --ignore-path .gitignore .",
    "lint:ci": "yarn run eslint --format checkstyle --ignore-path .gitignore .",
    "test": "yarn run jest"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^9.0.1",
    "jest": "^26.3.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-junit": "^11.1.0",
    "path": "^0.12.7",
    "process": "^0.11.10",
    "rollup": "^2.23.1",
    "rollup-plugin-terser": "^7.0.0",
    "rollup-plugin-typescript2": "^0.27.2",
    "ts-jest": "^26.2.0",
    "tslib": "^2.0.1",
    "typescript": "~3.9.7"
  }
}
