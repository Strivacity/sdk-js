{
  "private": true,
  "name": "@strivacity/source",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm exec simple-git-hooks",
    "build:all": "nx run-many --target=build",
    "build": "nx run-many --target=build --projects=$(nx show projects | grep '^sdk-' | tr '\n' ',' | sed 's/,$//')",
    "test": "nx run-many --target=test",
    "test:ci": "nx run-many --target=test --coverage.enabled",
    "lint": "nx run-many --target=eslint,stylelint",
    "release": "nx release --skip-publish"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec commitlint --edit $1",
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.12.0",
    "@analogjs/vitest-angular": "^1.12.0",
    "@angular-builders/custom-webpack": "^19.0.0",
    "@angular-devkit/architect": "~0.1900.7",
    "@angular-devkit/build-angular": "~19.0.7",
    "@angular-devkit/core": "~19.0.7",
    "@angular-devkit/schematics": "~19.0.7",
    "@angular-eslint/eslint-plugin": "~19.0.2",
    "@angular-eslint/eslint-plugin-template": "~19.0.2",
    "@angular-eslint/template-parser": "~19.0.2",
    "@angular/animations": "~19.0.6",
    "@angular/cli": "~19.0.7",
    "@angular/common": "~19.0.6",
    "@angular/compiler": "~19.0.6",
    "@angular/compiler-cli": "~19.0.6",
    "@angular/core": "~19.0.6",
    "@angular/forms": "~19.0.6",
    "@angular/platform-browser": "~19.0.6",
    "@angular/platform-browser-dynamic": "~19.0.6",
    "@angular/router": "~19.0.6",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.17.0",
    "@lit-labs/router": "^0.1.3",
    "@nuxt/devtools": "^1.7.0",
    "@nuxt/devtools-kit": "^1.7.0",
    "@nuxt/kit": "^3.15.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.1",
    "@nuxt/test-utils": "^3.15.1",
    "@nx/angular": "20.3.1",
    "@nx/eslint": "20.3.1",
    "@nx/js": "20.3.1",
    "@nx/next": "20.3.1",
    "@nx/nuxt": "20.3.1",
    "@nx/react": "20.3.1",
    "@nx/vite": "20.3.1",
    "@nx/vue": "20.3.1",
    "@nx/web": "20.3.1",
    "@nx/workspace": "20.3.1",
    "@remix-run/dev": "^2.15.2",
    "@remix-run/node": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "@remix-run/serve": "^2.15.2",
    "@schematics/angular": "~19.0.7",
    "@strivacity/sdk-core": "*",
    "@strivacity/vitest": "*",
    "@swc-node/register": "~1.10.9",
    "@swc/core": "~1.10.7",
