{
  "private": true,
  "name": "@strivacity/source",
  "version": "0.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "postinstall": "pnpm exec simple-git-hooks",
    "build:all": "nx run-many --target=build",
    "build": "nx run-many --target=build --projects=$(nx show projects | grep '^sdk-' | tr '\n' ',' | sed 's/,$//')",
    "test": "nx run-many --target=test",
    "test:ci": "nx run-many --target=test --coverage.enabled",
    "lint": "nx run-many --target=eslint,stylelint",
    "release": "nx release --skip-publish"
  },
  "simple-git-hooks": {
    "commit-msg": "pnpm exec commitlint --edit $1",
    "pre-commit": "pnpm exec lint-staged --concurrent false"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "*.{js,mjs,jsx,ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss}": [
      "stylelint --fix"
    ],
    "*.vue": [
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "devDependencies": {
    "@analogjs/vite-plugin-angular": "^1.8.0",
    "@analogjs/vitest-angular": "^1.8.0",
    "@angular-builders/custom-webpack": "^18.0.0",
    "@angular-devkit/build-angular": "~18.2.4",
    "@angular-devkit/core": "~18.2.4",
    "@angular-devkit/schematics": "~18.2.4",
    "@angular-eslint/eslint-plugin": "^18.3.1",
    "@angular-eslint/eslint-plugin-template": "^18.3.1",
    "@angular-eslint/template-parser": "^18.3.1",
    "@angular/animations": "~18.2.4",
    "@angular/cli": "~18.2.4",
    "@angular/common": "~18.2.4",
    "@angular/compiler": "~18.2.4",
    "@angular/compiler-cli": "~18.2.4",
    "@angular/core": "~18.2.4",
    "@angular/forms": "~18.2.4",
    "@angular/platform-browser": "~18.2.4",
    "@angular/platform-browser-dynamic": "~18.2.4",
    "@angular/router": "~18.2.4",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@lit-labs/router": "^0.1.3",
    "@nuxt/devtools": "^1.4.2",
    "@nuxt/devtools-kit": "^1.4.2",
    "@nuxt/kit": "^3.13.1",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.13.2",
    "@nuxt/test-utils": "^3.14.2",
    "@nx/angular": "19.7.3",
    "@nx/eslint": "19.7.3",
    "@nx/eslint-plugin": "19.7.3",
    "@nx/js": "19.7.3",
    "@nx/next": "19.7.3",
    "@nx/nuxt": "19.7.3",
    "@nx/react": "19.7.3",
    "@nx/vite": "19.7.3",
    "@nx/vue": "19.7.3",
    "@nx/web": "19.7.3",
    "@nx/workspace": "19.7.3",
    "@remix-run/dev": "^2.12.0",
    "@remix-run/node": "^2.12.0",
    "@remix-run/react": "^2.12.0",
    "@remix-run/serve": "^2.12.0",
    "@schematics/angular": "~18.2.4",
    "@strivacity/sdk-core": "*",
    "@strivacity/vitest": "*",
    "@swc-node/register": "~1.10.9",
    "@swc/core": "~1.7.26",
