    "@eslint/js": "^9.39.1",
    "@lit-labs/router": "^0.1.4",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/devtools-kit": "^3.1.1",
    "@nuxt/kit": "^4.2.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@nuxt/test-utils": "^3.21.0",
    "@nx/angular": "22.1.3",
    "@nx/eslint": "22.1.3",
    "@nx/js": "22.1.3",
    "@nx/next": "22.1.3",
    "@nx/react": "22.1.3",
    "@nx/vue": "22.1.3",
    "@nx/web": "22.1.3",
    "@nx/workspace": "22.1.3",
    "@remix-run/dev": "^2.17.2",
    "@remix-run/node": "^2.17.2",
    "@remix-run/react": "^2.17.2",
    "@remix-run/serve": "^2.17.2",
    "@schematics/angular": "~19.2.14",
    "@strivacity/sdk-core": "*",
    "@strivacity/testing": "*",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/vue": "^8.1.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-istanbul": "^4.0.15",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-vue": "^10.6.2",
    "glob": "^13.0.0",
    "globals": "^16.5.0",
    "happy-dom": "^20.0.11",
    "lint-staged": "^16.2.7",
    "lit": "^3.3.1",
    "luxon": "^3.7.2",
    "msw": "^2.12.4",
    "next": "~16.0.7",
    "ng-packagr": "^19.2.2",
    "nuxi": "^3.31.1",
    "nuxt": "^4.2.1",
    "nx": "22.1.3",
    "nx-stylelint": "^18.0.2",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.7.4",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-router": "^7.10.1",
    "remix-utils": "^9.0.0",
    "rxjs": "~7.8.2",
    "sass": "^1.94.2",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.26.1",
    "stylelint-config-html": "^1.1.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "ts-node": "10.9.2",
    "tslib": "^2.8.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.48.1",
    "urlpattern-polyfill": "^10.1.0",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.15",
    "vue": "^3.5.25",
    "vue-router": "^4.5.1",
    "vue-tsc": "^3.1.7",
    "webpack": "~5.99.9",
    "zone.js": "~0.16.0"
  },
  "pnpm": {
    "overrides": {
      "@strivacity/sdk-angular": "workspace:*",
      "@strivacity/sdk-core": "workspace:*",
      "@strivacity/sdk-next": "workspace:*",
      "@strivacity/sdk-nuxt": "workspace:*",
      "@strivacity/sdk-react": "workspace:*",
      "@strivacity/sdk-remix": "workspace:*",
      "@strivacity/sdk-vue": "workspace:*",
      "@strivacity/testing": "workspace:*"
    },
    "ignoredBuiltDependencies": [
      "lmdb",
      "msgpackr-extract",
      "sharp"
    ],
    "onlyBuiltDependencies": [
      "esbuild",
      "msw",
      "nx",
      "simple-git-hooks"
    ]
  }
}
