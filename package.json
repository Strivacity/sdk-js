    "@angular/platform-browser-dynamic": "~18.2.4",
    "@angular/router": "~18.2.4",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/js": "9.20.0",
    "@lit-labs/router": "^0.1.3",
    "@nuxt/devtools": "^2.0.0",
